<root main_tree_to_execute="MissionTree" BTCPP_format="4">
  <BehaviorTree ID="MissionTree">
    <Sequence name="Mission">
      <RetryUntilSuccessful num_attempts="-1" name="FindOrigin">
        <FindTag tag_id="0" tag_frame_name="tag_0" output_pose="{origin_pose}"/>
      </RetryUntilSuccessful>
      <RetryUntilSuccessful num_attempts="-1" name="FindDestination">
        <Sequence>
          <Spin speed="0.4" duration="10.0"/>
          <FindTag tag_id="5" tag_frame_name="tag_5" output_pose="{destination_pose}"/>
        </Sequence>
      </RetryUntilSuccessful>
      <!-- GoHome action will be added here later -->
      <AlwaysSuccess name="MissionComplete"/>
    </Sequence>
  </BehaviorTree>
</root>
